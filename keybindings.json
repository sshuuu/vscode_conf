// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
  // キャレットを先頭に移動
  {
    "key": "ctrl+a",
    "command": "cursorHome",
    "when": "editorTextFocus"
  },
  // キャレットを末尾に移動
  {
    "key": "ctrl+e",
    "command": "cursorEnd",
    "when": "editorTextFocus"
  },
  // ワークベンチのペインサイズを最大化/最小化
  {
    "key": "cmd+shift+a",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  // ターミナルの表示/非表示
  {
    "key": "ctrl+t",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  // 次のターミナルにフォーカスを移動
  {
    "key": "cmd+left",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  // 前のターミナルにフォーカスを移動
  {
    "key": "cmd+right",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  // 新規ターミナルの作成
  {
    "key": "cmd+t",
    "command": "workbench.action.terminal.new",
    "when": "terminalFocus"
  },
  // （既定上書き）
  // ターミナルにフォーカスされていなければワークスペース内のシンボルへ移動
  {
    "key": "cmd+t",
    "command": "-workbench.action.showAllSymbols"
  }, {
    "key": "cmd+t",
    "command": "workbench.action.showAllSymbols",
    "when": "!terminalFocus"
  },
  // アクティブなターミナルを閉じる
  {
    "key": "cmd+w",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
  // (規定上書き)
  // ターミナルにフォーカスされていなければエディターを閉じる
  {
    "key": "cmd+w",
    "command": "-workbench.action.closeActiveEditor"
  },{
    "key": "cmd+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "!terminalFocus"
  }
]